<?xml version="1.0" encoding="UTF-8"?>
<ColView PrefWidth="800" Spacing="5" FillWidth="true">
  <font name="Arial" size="15" />
  <RowView Align="CENTER" Padding="4" GrowWidth="true" Spacing="4">
    <BevelBorder />
    <color value="#3dcece" />
    <Label image="Header.png" />
  </RowView>
  <RowView Name="TextRowView" GrowHeight="true" FillHeight="true">
    <TextView Name="ScriptText" PrefHeight="300" Padding="5" GrowWidth="true">
      <font name="Arial" size="20" />
    </TextView>
  </RowView>
  <RowView Padding="3" Spacing="5">
    <Button Name="GifButton" PrefWidth="100" text="Make GIF" />
    <Button Name="RunButton" PrefWidth="100" LeanX="RIGHT" text="Run" />
  </RowView>
</ColView>